<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <AssemblyName>neon-de</AssemblyName>
    <AssemblyTitle>Neo.Compiler.MSIL</AssemblyTitle>
    <Authors>The Neo Project</Authors>
    <Company>The Neo Project</Company>
    <Copyright>2015-2019 The Neo Project</Copyright>
    <Description>This is a fork of the official Neo.Compiler.MSIL compiler with debugger enhancements.</Description>
    <OutputType>Exe</OutputType>
    <PackageIcon>neo-logo-72.png</PackageIcon>
    <PackageId>Neo.neon-de</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/devhawk/neo-devpack-dotnet</PackageProjectUrl>
    <PackageTags>NEO;Blockchain;Smart Contract;Compiler</PackageTags>
    <PackAsTool>true</PackAsTool>
    <Product>Neo.Compiler.MSIL</Product>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/devhawk/neo-devpack-dotnet.git</RepositoryUrl>
    <RootNamespace>Neo.Compiler</RootNamespace>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>	
    <TargetFramework>netcoreapp2.2</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <None Include="neo-logo-72.png" Pack="true" Visible="false" PackagePath="" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Mono.Cecil" Version="0.10.3" />
    <PackageReference Include="Neo.VM" Version="2.4.1" />
  </ItemGroup>

</Project>
